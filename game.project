[project]
title = OverrunRoom
version = 0.1
dependencies = https://github.com/britzl/ludobits/archive/1.2.zip, https://github.com/britzl/defold-orthographic/archive/1.1.zip, https://github.com/britzl/defold-timer/archive/master.zip

[bootstrap]
main_collection = /oneroom/oneroom.collectionc
render = /render/orthographic.renderc

[input]
game_binding = /input/game.input_bindingc

[display]
width = 1280
height = 960

[physics]
scale = 0.02
debug = 0
max_collisions = 200
max_contacts = 100

[script]
shared_state = 1

[ios]
bundle_identifier = com.bitfront.oneroom

[android]
package = com.bitfront.oneroom

[osx]
bundle_identifier = com.bitfront.oneroom

[graphics]
default_texture_min_filter = nearest
default_texture_mag_filter = nearest

[factory]
max_count = 1000

[sprite]
max_count = 500

[particle_fx]
max_count = 300
max_particle_count = 1000
